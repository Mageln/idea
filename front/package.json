{
  "name": "@webapp/front",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint --cache --cache-location ./node_modules/.cache .",
    "preview": "vite preview",
    "stylecheck": "sass --update -q ./src:. /node_modules/.cache/.stylecheckcache",
    "prettify": "prettier --loglevel warn --cache '{src/**/*,*}.{ts,tsx,json,yml,scss}' !**/{node_modules,dist}/**/*",
    "types": "tsc --noEmit --project ./tsconfig.json && tsc --noEmit --project ./tsconfig.node.json"
  },
  "dependencies": {
    "@csstools/normalize.css": "^12.1.1",
    "@tanstack/react-query": "^4.36.1",
    "@trpc/client": "^10.45.2",
    "@trpc/react-query": "^10.45.2",
    "@webapp/back": "workspace:*",
    "formik": "^2.4.6",
    "formik-validator-zod": "^2.0.1",
    "include-media": "^2.0.0",
    "postcss-normalize": "^13.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.0.2",
    "reset-css": "^5.0.2",
    "sass": "^1.82.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@sentry/vite-plugin": "^2.7.1",
    "@types/js-cookie": "^3.0.3",
    "@types/lodash": "^4.14.198",
    "@types/mixpanel-browser": "^2.47.3",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/react-image-gallery": "^1.2.1",
    "@types/react-infinite-scroller": "^1.2.3",
    "@vitejs/plugin-legacy": "^4.1.1",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.16",
    "prettier": "^3.4.2",
    "rollup-plugin-visualizer": "^5.9.2",
    "stylelint": "^15.11.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard-scss": "^11.1.0",
    "vite": "^4.4.9",
    "vite-plugin-svgr": "^3.2.0"
  }
}
